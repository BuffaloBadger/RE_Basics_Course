@incollection{wheeler_reaction_1951,
	title = {Reaction {Rates} and {Selectivity} in {Catalyst} {Pores}},
	volume = {3},
	url = {https://www.sciencedirect.com/science/article/pii/S0360056408601091},
	abstract = {This chapter focuses on the reaction rates and selectivity in catalyst pores; and discusses the physical picture of the pore structure, the mechanism of transport in catalyst pores, and physical factors determining reaction rates on porous catalysts. Small pores appear to have fundamentally different catalytic properties, due to the peculiar way in which transport into and out of the catalyst pellet affects the reactivity of the catalyst, through its control of the partial pressure of reactant in the inner pore recesses. A catalyst pore is essentially a microscopic flow reactor. As molecules flow into a pore (mainly by diffusion), reaction occurs due to collisions with the pore wall and the partial pressure of reactant decreases, just as in a bulk scale flow reactor. Thus, the partial pressure of a reactant is in general smaller in the inner recesses of a catalyst pellet than at the pellet surface, just as the partial pressure of a reactant is lower at the outlet end of a bulk reactor than it is at the inlet end. From this simple phenomenon a large number of interesting deductions are possible.},
	language = {en},
	urldate = {2023-04-26},
	booktitle = {Advances in {Catalysis}},
	publisher = {Academic Press},
	author = {Wheeler, Ahlborn},
	editor = {Frankenburg, W. G. and Komarewsky, V. I. and Rideal, E. K. and Emmett, P. H. and Taylor, H. S.},
	month = jan,
	year = {1951},
	doi = {10.1016/S0360-0564(08)60109-1},
	pages = {249--327},
	file = {ScienceDirect Snapshot:/Users/lund/Zotero/storage/AKSM8CSQ/S0360056408601091.html:text/html},
}

